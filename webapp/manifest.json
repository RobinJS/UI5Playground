{
	"_version": "1.58.0",
	"sap.app": {
		"id": "ui5.playground",
		"i18n": {
			"bundleUrl": "i18n/i18n.properties",
			"supportedLocales": [
				""
			],
			"fallbackLocale": ""
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"type": "application",
		"applicationVersion": {
			"version": "1.0.0"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"dependencies": {
			"minUI5Version": "1.108.0",
			"libs": {
				"sap.m": {},
				"sap.ui.core": {}
			}
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "ui5.playground.i18n.i18n",
					"supportedLocales": [
						""
					],
					"fallbackLocale": ""
				}
			},
            "ProductsModel": {
                "type" : "sap.ui.model.json.JSONModel"
            }
		},
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
		"rootView": {
			"viewName": "ui5.playground.view.App",
			"type": "XML",
			"id": "app"
		},
		"config": {
			"sample": {
				"files": [
					"App.view.xml",
					"Home.view.xml",
					"Storage.view.xml",
					"App.controller.js",
					"Home.controller.js",
					"Storage.controller.js",
					"manifest.json"
				]
			}
		},
        "routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"type": "View",
				"viewType": "XML",
				"path": "ui5.playground.view",
				"controlId": "app",
				"controlAggregation": "pages",
				"transition": "slide",
				"bypassed": {
					"target": "notFound"
				}
			},
			"routes": [{
				"pattern": "",
				"name": "app",
				"target": "app"
			}, {
				"pattern": "home",
				"name": "home",
				"target": "home"
			}, {
				"pattern": "recipes",
				"name": "recipes",
				"target": "recipes"
			}, {
				"pattern": "storage",
				"name": "storage",
				"target": "storage"
			}],
			"targets": {
                "app": {
					"id": "app",
					"name": "App"
				},
				"home": {
					"id": "home",
					"name": "Home"
				},
				"notFound": {
					"id": "notFound",
					"name": "NotFound",
					"transition": "show"
				},
                "recipes": {
                    "id": "recipes",
                    "path": "recipes",
                    "name": "recipes"
                },
				"storage": {
					"id": "storage",
					"path": "storage",
					"name": "storage"
				},
                "order": {
					"id": "order",
					"path": "order",
					"name": "order"
				}

			}
		}
	}
}
